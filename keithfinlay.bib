 month = {2014/08/21},
 number = {10},
 pages = {1747-52},
 title = {Revolving Doors: Imprisonment among the Homeless and Marginally Housed Population},
 url = {http://dx.doi.org/10.2105/AJPH.2005.065094},
 volume = {95},
 year = {2005}
}

