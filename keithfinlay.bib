 month = {8},
 note = {Software published 13 August 2013 on \emph{Statistical Software Components} at ̆rl{http://ideas.repec.org/c/boc/bocode/s457684.html}. Last updated 4 October 2014},
 title = {{weakiv}: Stata Module to Perform Weak-Instrument-Robust Tests and Confidence Intervals for Instrumental-Variable (IV) Estimation of Linear, Probit, and Tobit Models},
