 title = {The Edge of Stigma: An Experimental Audit of the Effects of Low-Level Criminal Records on Employment},
 url = {http://dx.doi.org/10.1111/1745-9125.12051},
 volume = {52},
 year = {2014}
}

@article{Gneezy2000A-Fine-Is-a-Pri,
 abstract = {Abstract The deterrence hypothesis predicts that the introduction of a penalty that leaves everything else unchanged will reduce the occurrence of the behavior subject to the fine. We present the result of a field study in a group of day‐care centers that contradicts this prediction. Parents used to arrive late to collect their children, forcing a teacher to stay after closing time. We introduced a monetary fine for late‐coming parents. As a result, the number of late‐coming parents increased significantly. After the fine was removed no reduction occurred. We argue that penalties are usually introduced into an incomplete contract, social or private. They may change the information that agents have, and therefore the effect on behavior may be opposite of that expected. If this is true, the deterrence hypothesis loses its predictive strength, since the clause ``everything else is left unchanged'' might be hard to satisfy.},
 author = {Uri Gneezy and Aldo Rustichini},
 date-added = {2014-07-09 16:33:41 +0000},
 date-modified = {2014-07-09 16:34:00 +0000},
 journal = {Journal of Legal Studies},
