 year = {2011}
}

@article{Damm2009Determinants-of,
 abstract = {This paper exploits a Danish spatial dispersal policy on refugees, which can be regarded as a natural experiment to investigate the influence of regional factors on recent immigrants' location choices. The main push factors are lack of conationals and immigrants. Additional push factors are lack of rental, including social, housing and lack of institutions for qualifying education, which explain why recent immigrants are attracted to large cities. Finally, placed refugees tend to leave locations with relatively high regional unemployment, ancj there is indirect evidence of welfare seeking.},
