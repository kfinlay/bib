@article{Guthrie2013Streetcars-and-,
 abstract = {Researching traditional streetcars' development impacts is challenging: most U.S. lines operate in downtown areas with many development stimuli. This article addresses that challenge through analysis of New Orleans building permits after Hurricane Katrina. We estimate how post-Katrina permit frequency changes with distance from streetcar stops, controlling for damage, proximity to commercial areas, and pre-Katrina demographics. We find that distance to stops strongly predicts building permits. Residential permits increase with distance to stops; commercial permits decrease. Findings confirm streetcars support commercial development, yet suggest potential displacement of residential uses. Implications for future streetcar projects in New Orleans and elsewhere are discussed.},
 author = {Guthrie, Andrew and Fan, Yingling},
