 keyword = {Criminal behavior, crystal methamphetamine, drug use behaviors, methamphetamine, sexual risk behavior, young adults},
 number = {7},
 pages = {1102-13},
 title = {Crystal Methamphetamine Use Among Young Adults in the USA},
 url = {http://dx.doi.org/10.1111/j.1360-0443.2007.01847.x},
 volume = {102},
 year = {2007}
}

