 volume = {107},
 year = {1999}
}

@article{Neumark2003Minimum-Wages-a,
 abstract = {Although economic research on minimum wages has focused predominantly on employment effects, some attention has been devoted to the analysis of the effects of minimum wages on skill acquisition and education. This paper takes another look at evidence on the effects of minimum wages on schooling, seeking to reconcile some of the contradictory results in recent research, using CPS data from the late 1970s through the 1980s. Based on this re-examination, as well as on additional results updating the data through 1998, we conclude that the evidence points to negative effects of minimum wages on school enrollment, bolstering the findings of negative effects of minimum wages on enrollment from a number of recent studies, as well as some older ones, and countering claims that appeared to overturn at least some of this evidence.},
 author = {David Neumark and William Wascher},
 date-added = {2011-11-01 01:04:36 +0000},
