@article{Oettinger1996Statistical-Dis,
 abstract = {This article develops and tests a simple dynamic model of statistical discrimination. The model improves on earlier static models both by allowing ex ante uncertainty about worker productivity to be resolved as on-the-job performance is observed and by generating several testable empirical implications. These predictions are tested using a sample of young men from the National Longitudinal Survey of Youth, producing mixed evidence for the model. The main empirical result is that no black-white wage gap exists at labor force entry but that one develops as experience accumulates, mainly because blacks reap smaller gains from job mobility.},
 annote = {Develops two-period dynamic model of statistical discrimination with learning and job-change. Finds that, as the precision of a skill signal increases, the measured return to experience increases and the measured return to job tenure decreases. Thus, if blacks had less precise skill signals (as posited in Aigner and Cain 1977), the model unambiguously predicts that blacks should have a higher measured return to job tenure and lower measured return to general experience than whites. This leads to an inefficiently low rate of job change for blacks, which creates a gap in wages between blacks and whites over time. This follows from the assumption that wage increases occur throuhg job change. Applies this to a restricted sample of the NLSY, and finds that the wage gap is negligible at first entry, and increases over time in the labor market. Finds less benefit of experience for blacks, but less strong evidence with the measurement of returns to tenure. Could be the very short sample period.},
 author = {Oettinger, Gerald S.},
 date-modified = {2012-12-04 19:35:23 +0000},
 dateread = {28 December 2005},
 doi = {10.1086/209803},
 journal = {Journal of Labor Economics},
 keyword = {statisticaldiscrimination dynamic race wagegap learning; Untitled},
 month = {1},
 number = {1},
 pages = {52-78},
 title = {Statistical Discrimination and the Early Career Evolution of the Black-White Wage Gap},
 url = {http://dx.doi.org/10.1086/209803},
 volume = {14},
 year = {1996}
}

