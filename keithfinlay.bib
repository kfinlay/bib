@incollection{DiNardo2011Program-Evaluat,
 abstract = {This chapter provides a selective review of some contemporary approaches to program evaluation. One motivation for our review is the recent emergence and increasing use of a particular kind of â€œprogramâ€ in applied microeconomic research, the so-called Regression Discontinuity (RD) Design of Thistlethwaite and Campbell (1960). We organize our discussion of these various research designs by how they secure internal validity: in this view, the RD design can been seen as a close â€œcousinâ€ of the randomized experiment. An important distinction which emerges from our discussion of â€œheterogeneous treatment effectsâ€ is between ex post (descriptive) and ex ante (predictive) evaluations; these two types of evaluations have distinct, but complementary goals. A second important distinction we make is between statistical statements that are descriptions of our knowledge of the program assignment process and statistical statements that are structural assumptions about individual behavior. Using these distinctions, we examine some commonly employed evaluation strategies, and assess them with a common set of criteria for â€œinternal validityâ€, the foremost goal of an ex post evaluation. In some cases, we also provide some concrete illustrations of how internally valid causal estimates can be supplemented with specific structural assumptions to address â€œexternal validityâ€: the estimate from an internally valid â€œexperimentalâ€ estimate can be viewed as a â€œleading termâ€ in an extrapolation for a parameter of interest in an ex ante evaluation.},
 author = {DiNardo, John E. and Lee, David S.},
 booktitle = {Handbook of Labor Economics},
