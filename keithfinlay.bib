@unpublished{Frandsen2010Quantile-Treatm,
 abstract = {We introduce a nonparametric estimator for local quantile treatment e{\S}ects in the regression discontinuity (RD) design. The procedure uses local distribution regression to estimate the marginal distributions of the potential outcomes. The estimator attains the optimal rate of convergence for one-dimensional nonparametric regression and is asymptotically normal. We illustrate the procedure through Monte Carlo simulations and an application on the distributional e{\S}ects of a universal pre-K program in Okla- homa. We Önd that participation in a pre-K program signiÖcantly raises the lower end and the middle of the distribution of test scores.},
