 keyword = {Cost benefit analysis ; Volunteer programs ; Cost analysis ; Court volunteers ; Youth community involvement ; OJJDP grant-related documents ; Teen courts},
 month = {5},
 note = {Office of Juvenile Justice and Delinquency Prevention, {NCJ} 222592},
 title = {Youth Courts: An Empirical Update and Analysis of Future Organizational and Research Needs},
 url = {http://hamfish.org/Publications/Serial/HFI_Youth_Courts_Report.pdf},
 year = {2008}
}

