@article{Singer1988Criminalizing-D,
 abstract = {New York's Juvenile Offender (JO) Law of 1978 is a significant step away from separate systems of justice for adults and juveniles. The law requires that juveniles accused of violent offenses be tried in criminal court, and it provides penalties comparable to those for adults. This paper evaluates the impact of the JO Law on violent juvenile crime rates in New York City and in upstate New York. Analyzing arrest data through the use of an interrupted time series model, we conclude that the JO Law has not been effective in reducing juvenile crime.},
