@article{Lane2002Adult-Versus-Ju,
 abstract = {This article reports findings from face-to-face interviews with youthful offenders in Florida, about half of whom had been transferred to the adult system and half of whom were retained in the juvenile system. The focus is on the youths' global assessments of the impact of their correctional experiences relevant to subsequent offending. The overall impact of each recalled correctional disposition was rated (ranging from beneficial impact to negative impact). For respondents who had experienced multiple correctional dispositions, comparisons were made about the relative impact of low-end versus deep-end juvenile commitments and juvenile versus adult sanctions. Youths believed deep-end juvenile placements were most beneficial. Those programs were viewed as having provided education or life skills. When youths viewed adult sanctions as being beneficial, the benefit was linked to the time and pain of prison confinement. Those youths who attributed positive impact to prison had ``skipped'' deep-end juvenile placements.},
